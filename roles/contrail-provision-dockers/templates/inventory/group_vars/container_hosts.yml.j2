CONTAINER_REGISTRY: "{{ docker_registry.fqdn }}:{{ docker_registry.port }}"
contrail_configuration:
  CONTRAIL_VERSION: "{{ contrail_version }}"
  OPENSTACK_VERSION: "{{ openstack_version }}"
  CONTROLLER_NODES: "{{ hostvars[inventory_hostname]['ansible_host'] }}"
  CLOUD_ORCHESTRATOR: kubernetes
roles:
  "{{ hostvars[inventory_hostname]['ansible_host'] }}":
    configdb:
    config:
    control:
    webui:
    analytics:
    analyticsdb:
    k8s_master:
    vrouter:
